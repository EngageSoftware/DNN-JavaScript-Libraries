<dotnetnuke type="Package" version="5.0">
  <packages>
    <package name="jquery.scrollTo" type="JavaScript_Library" version="2.1.2">
      <friendlyName>jQuery.scrollTo</friendlyName>
      <description>A small, customizable plugin for scrolling elements, or the window itself</description>
      <owner>
        <name>Engage Software</name>
        <organization>Engage Software</organization>
        <url>http://www.engagesoftware.com</url>
        <email>support@engagesoftware.com</email>
      </owner>
      <license src="LICENSE.htm" />
      <releaseNotes src="CHANGES.htm" />
      <azureCompatible>true</azureCompatible>
      <dependencies>
        <dependency type="managedPackage" version="1.8.0">jQuery</dependency>
      </dependencies>
      <components>
        <component type="JavaScript_Library">
          <javaScriptLibrary>
            <libraryName>jquery.scrollTo</libraryName>
            <fileName>jquery.scrollTo.min.js</fileName>
            <preferredScriptLocation>BodyBottom</preferredScriptLocation>
            <CDNPath>https://cdn.jsdelivr.net/npm/jquery.scrollto@2.1.2/jquery.scrollTo.min.js</CDNPath>
            <objectName>jQuery.scrollTo</objectName>
          </javaScriptLibrary>
        </component>
        <component type="JavaScriptFile">
          <jsfiles>
            <libraryFolderName>jquery.scrollTo</libraryFolderName>
            <jsfile>
              <name>jquery.scrollTo.min.js</name>
            </jsfile>
          </jsfiles>
        </component>
      </components>
    </package>
  </packages>
</dotnetnuke>
