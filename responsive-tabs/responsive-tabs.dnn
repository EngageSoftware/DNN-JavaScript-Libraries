<dotnetnuke type="Package" version="5.0">
  <packages>
    <package name="responsive-tabs" type="JavaScript_Library" version="<~=version~>">
      <friendlyName>jQuery Responsive Tabs</friendlyName>
      <description><![CDATA[
<p>This jQuery plugin provides responsive tab functionality. The tabs transform to an accordion when it reaches a CSS breakpoint.
Check out a demo at <a href="http://jellekralt.github.io/Responsive-Tabs/demo.html">http://jellekralt.github.io/Responsive-Tabs/demo.html</a></p>
      ]]></description>
      <owner>
        <name>Engage Software</name>
        <organization>Engage Software</organization>
        <url>http://www.engagesoftware.com</url>
        <email>support@engagesoftware.com</email>
      </owner>
      <license src="LICENSE.htm" />
      <releaseNotes src="CHANGES.htm" />
      <azureCompatible>true</azureCompatible>
      <dependencies>
        <dependency type="managedPackage" version="1.7.2">jQuery</dependency>
      </dependencies>
      <components>
        <component type="JavaScript_Library">
          <javaScriptLibrary>
            <libraryName>responsive-tabs</libraryName>
            <fileName>jquery.responsiveTabs.min.js</fileName>
            <preferredScriptLocation>BodyBottom</preferredScriptLocation>
            <CDNPath>https://cdn.jsdelivr.net/npm/responsive-tabs@<~=version~>/js/jquery.responsiveTabs.min.js</CDNPath>
            <objectName></objectName>
          </javaScriptLibrary>
        </component>
        <component type="JavaScriptFile">
          <jsfiles>
            <libraryFolderName>responsive-tabs</libraryFolderName>
            <jsfile>
              <name>jquery.responsiveTabs.min.js</name>
            </jsfile>
          </jsfiles>
        </component>
        <component type="ResourceFile">
          <resourceFiles>
            <basePath>Resources\Libraries\responsive-tabs\<~=versionFolder~></basePath>
            <resourceFile>
              <name>Resources.zip</name>
            </resourceFile>
          </resourceFiles>
        </component>
      </components>
    </package>
  </packages>
</dotnetnuke>
